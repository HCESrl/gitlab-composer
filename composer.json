{
    "name":"hcesrl/gitlab-composer",
    "description":"A Composer repository index generator for your GitLab CE projects",
    "license":"MIT",
    "keywords":[
        "gitlab",
        "composer",
        "packages"
    ],
    "authors":[
        {
            "name":"Fabio Savina",
            "email":"fsavina@hce.it"
        }
    ],
    "require":{
        "m4tthumphrey/php-gitlab-api":"^9.6",
        "php-http/guzzle6-adapter":"^1.1|^2.0"
    },
    "require-dev":{
        "filp/whoops":"~2.0",
        "phpunit/phpunit":"~7.0"
    },
    "autoload":{
        "psr-4":{
            "GitLabComposer\\":"src/GitLabComposer"
        }
    }
}
